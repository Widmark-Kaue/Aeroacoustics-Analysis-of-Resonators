FoamFile
{
    version     1812;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType  laminar; //RAS; // laminar

RAS
{
    RASModel        kOmegaSST; // turbulenve model for tubes

    turbulence      on;

    printCoeffs     on;
}

